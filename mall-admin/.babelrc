{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    ["es2015", { "modules": false }],
    "stage-2"
  ],
  "plugins": ["syntax-dynamic-import", "transform-runtime", "transform-vue-jsx"],
  "comments": false,
  "env": {
    "test": {
      "plugins": ["istanbul"]
    },
    "production": {
      "plugins": ["transform-react-constant-elements"]
    }
  }
}